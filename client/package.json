{
  "name": "gym-manager-client",
  "private": true,
  "version": "1.0.0",
  "description": "gym manager client",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode production",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "preview:prod": "vite preview --mode production",
    "firebase:build:staging": "npm run build:staging",
    "firebase:build:prod": "npm run build:prod",
    "firebase:deploy:staging": "firebase use staging && firebase deploy --only hosting:client",
    "firebase:deploy:prod": "firebase use production && firebase deploy --only hosting:client",
    "firebase:preview:staging": "npm run firebase:build:staging && firebase hosting:channel:deploy staging --only hosting:client",
    "firebase:preview:prod": "npm run firebase:build:prod && firebase hosting:channel:deploy production --only hosting:client",
    "lint": "echo 'No linting configured'"
  },
  "author": {
    "name": "Hector"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "apexcharts": "^3.37.0",
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "date-fns": "^4.1.0",
    "dropzone": "^6.0.0-beta.2",
    "flatpickr": "^4.6.13",
    "js-cookie": "^3.0.5",
    "jsvectormap": "^1.5.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "nouislider": "^15.7.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.13",
    "react-hot-toast": "^2.4.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-phone-number-input": "^3.4.9",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.8.1",
    "react-select-country-list": "^2.2.3",
    "react-table": "^7.8.0",
    "sort-by": "^0.0.2",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@types/dropzone": "^5.7.8",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-select-country-list": "^2.2.3",
    "@types/react-table": "^7.7.15",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.13",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "file-loader": "^6.2.0",
    "postcss": "^8.4.21",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.1",
    "vite": "^6.2.0",
    "webpack": "^5.79.0"
  },
  "paths": {
    "@shared/*": [
      "../shared/*"
    ]
  }
}
